{
  "positive": [
    {"text": "Har kun — yangi imkoniyat.", "author": "Motivatsiya"},
    {"text": "Tabassum kayfiyatni tez o‘zgartiradi.", "author": "Motivatsiya"},
    {"text": "Ijobiy fikr — ijobiy hayot.", "author": "Motivatsiya"},
    {"text": "Dunyo sen o‘zgarganda o‘zgaradi.", "author": "Motivatsiya"},
    {"text": "Yorug‘lik ichingda — uni so‘ndirma.", "author": "Motivatsiya"},
    {"text": "Eng oddiy kun ham mo‘jiza bo‘lishi mumkin.", "author": "Motivatsiya"}
  ],

  "love": [
    {"text": "Sevgi — dunyoni chiroyli qiladi.", "author": "Motivatsiya"},
    {"text": "Yurak biladigan narsani aql ba’zan tushunmaydi.", "author": "Motivatsiya"},
    {"text": "Samimiyat har qanday devorni buzadi.", "author": "Motivatsiya"},
    {"text": "Muhabbat — eng kuchli til.", "author": "Motivatsiya"},
    {"text": "Bir so‘z — butun qalbni davolashi mumkin.", "author": "Motivatsiya"},
    {"text": "Mehr berilgan joyda hayot gullaydi.", "author": "Motivatsiya"}
  ],

  "self_love": [
    {"text": "O‘zingni qadrlasang — boshqalar ham qadrlaydi.", "author": "Motivatsiya"},
    {"text": "Senga g‘amxo‘rlik kerak — bunga loyiqsan.", "author": "Motivatsiya"},
    {"text": "O‘zingni yaxshi ko‘r — bu kuchdir.", "author": "Motivatsiya"},
    {"text": "O‘zing yetarlicha yaxshisan.", "author": "Motivatsiya"},
    {"text": "Qadring boshqalarning fikri bilan o‘lchanmaydi.", "author": "Motivatsiya"},
    {"text": "Ruhiy salomatlik — birinchi o‘rinda bo‘lishi kerak.", "author": "Motivatsiya"}
  ],

  "strength": [
    {"text": "Senga kerak bo‘lgan kuch ichingda.", "author": "Motivatsiya"},
    {"text": "Og‘riq vaqtinchalik, kuch — abadiy.", "author": "Motivatsiya"},
    {"text": "Qiyinchiliklar seni kuchli qiladi.", "author": "Motivatsiya"},
    {"text": "Har qulash — yana turish imkoniyati.", "author": "Motivatsiya"},
    {"text": "Sabr — kuchning bir ko‘rinishi.", "author": "Motivatsiya"},
    {"text": "Bahona emas, sabab izla.", "author": "Motivatsiya"}
  ],

  "life": [
    {"text": "Hayot — yo‘l, manzil emas.", "author": "Motivatsiya"},
    {"text": "To‘xtash ham ba’zan rivojdir.", "author": "Motivatsiya"},
    {"text": "Onlarni qadrlang, ular qaytmaydi.", "author": "Motivatsiya"},
    {"text": "Har kun yangi sahifa.", "author": "Motivatsiya"},
    {"text": "Vaqt — eng qimmat boylik.", "author": "Motivatsiya"},
    {"text": "Hayot saboq orqali o‘rgatadi.", "author": "Motivatsiya"}
  ],

  "calm": [
    {"text": "Sokinlik — ruhning dori.", "author": "Motivatsiya"},
    {"text": "Nazorat — chuqur nafasdan boshlanadi.", "author": "Motivatsiya"},
    {"text": "Ba’zan eng to‘g‘ri qaror — kutish.", "author": "Motivatsiya"},
    {"text": "Ichki tinchlik — haqiqiy boylik.", "author": "Motivatsiya"},
    {"text": "Tinch yurak — baxtli hayat.", "author": "Motivatsiya"},
    {"text": "Shoshma — osuda qadam eng mustahkamidir.", "author": "Motivatsiya"}
  ],

  "relationships": [
    {"text": "Hurmat — munosabatlarning poydevori.", "author": "Motivatsiya"},
    {"text": "Eshitish — sevishning bir qismi.", "author": "Motivatsiya"},
    {"text": "Ishonch vaqt bilan quriladi.", "author": "Motivatsiya"},
    {"text": "Samimiyat yaqinlikni kuchaytiradi.", "author": "Motivatsiya"},
    {"text": "Chiroyli so‘z — ko‘ngilga nur beradi.", "author": "Motivatsiya"},
    {"text": "Muloqot — har qanday muammoning yechimi.", "author": "Motivatsiya"}
  ],

  "sad": [
    {"text": "Ko‘z yosh — qalbning yengilligi.", "author": "Motivatsiya"},
    {"text": "Qorong‘ilik yorug‘likni yorqinroq qiladi.", "author": "Motivatsiya"},
    {"text": "Hamma og‘riq vaqtinchalik.", "author": "Motivatsiya"},
    {"text": "Eng kuchlilar — eng ko‘p sinovdan o‘tganlar.", "author": "Motivatsiya"},
    {"text": "G‘am — o‘tkinchi, kuch — doimiy.", "author": "Motivatsiya"},
    {"text": "Har tunning ertalabi bor.", "author": "Motivatsiya"}
  ]
